<div>
    <h1 class="card z-depth-3 center-align">Get the latest news!</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' to get the latest articles</h5>
    </div>
</div>

<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align"></div>
    {{#each article}}
    <div class="card-panel">
        <h4>{{this.title}}</h4>
        <a href="/readArticle/{{this._id}}" class="waves-effects waves-light btn" id="btnSignUp">Read Articles</a>
    </div>
    {{/each}}
</div>